pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Tuchsanai', description: 'Enter your name')
        string(name: 'age', defaultValue: '20', description: 'Enter your age')

    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.age}"
                sh 'python myscript.py KITTIPOB --age 20'
            }
        }
    }
}
